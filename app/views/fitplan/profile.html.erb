<center>
	<h1>Profile Page</h1>
	<div id="message"><%= @message %></div>
	<a href="/profile/workout">Workout plan</a>
	<form action="/profile/add_food" method="post">
		<input id="food" name="food" type="text"><input id="search_food" type="submit" value="Add Food">
	</form>
	<% for entry in @entries %>
		<div style="display:inline-block">
			<div style="float:left; padding:10px;"><%= entry[0] %></div>
			<div style="float:left; padding:10px;"><%= entry[1] %> cal</div>
			<div style="float:left; padding:10px;"><%= entry[2] %></div>
		</div><br>
	<% end %>
</center>